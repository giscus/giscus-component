{
  "name": "@giscus/vue",
  "private": false,
  "description": "Type-safe Vue component for giscus",
  "version": "1.1.1",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "cz": "yarn --cwd ../.. cz",
    "commit": "yarn cz",
    "prepare": "yarn --cwd ../..",
    "dev": "vite",
    "lint": "vue-tsc --noEmit",
    "build": "run-p build:*",
    "build:script": "vite build",
    "build:types": "dts-bundle-generator --no-banner --no-check --external-imports=vue --external-types= -o dist/index.d.ts lib/index.ts",
    "serve": "vite preview"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.4",
    "@vitejs/plugin-vue-jsx": "^1.1.6",
    "@vue/compiler-sfc": "^3.1.4",
    "vue": "^3.1.4",
    "vue-tsc": "^0.2.0"
  },
  "author": {
    "name": "laymonage",
    "email": "me@laymonage.com",
    "url": "https://laymonage.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/giscus/giscus-component.git",
    "directory": "packages/vue"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "giscus",
    "comment",
    "comments",
    "comment-system",
    "vue",
    "github",
    "component",
    "discussions"
  ],
  "homepage": "https://github.com/giscus/giscus-component#readme",
  "bugs": "https://github.com/giscus/giscus-component/issues",
  "publishConfig": {
    "access": "public"
  }
}
